{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "YourAzureAdDomain.onmicrosoft.com",
    "TenantId": "your-aad-tenant-id",
    "ClientId": "app-registration-client-id",
    "CallbackPath": "/signin-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "App": {
    // Replace these with your own settings
    "KeyVaultKeyId": "https://KeyVaultName.vault.azure.net/keys/KeyName/KeyVersion",
    "StorageAccountBlobBaseUrl": "https://StorageAccountName.blob.core.windows.net",
    "StorageContainerName": "keys",
    "StorageBlobName": "testkeys.xml",
    // This is used in local development,
    // should be the id of AAD tenant connected to sub containing the KV and Storage
    "SharedTokenCacheTenantId": "aad-tenant-id"
  }
}
